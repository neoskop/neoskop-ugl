{
  "name": "@neoskop/ugl",
  "version": "1.2.1",
  "description": "A library to create UGL files",
  "main": "lib/index.js",
  "jsnext:main": "lib.esm/index.js",
  "typings": "lib/index.d.ts",
  "exports": {
    "types": "./lib/index.d.ts",
    "import": "./lib.esm/index.js",
    "default": "./lib/index.js"
  },
  "scripts": {
    "test:raw": "mocha",
    "test:watch": "npm run test:raw -- --watch --watch-extension ts",
    "test:coverage": "nyc -e '.ts' -r html -r text npm run test:raw",
    "test": "npm run test:coverage && nyc report -r text-lcov | coveralls",
    "clean": "rimraf lib lib.esm coverage",
    "prebuild": "npm run test:coverage",
    "build": "npm run clean && tsc && tsc -p tsconfig.esm.json && echo '{ \"type\": \"module\" }' > lib.esm/package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neoskop/neoskop-ugl"
  },
  "keywords": [
    "ugl"
  ],
  "author": "Mark Wecke <mark.wecke@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neoskop/neoskop-ugl/issues"
  },
  "homepage": "https://github.com/neoskop/neoskop-ugl#readme",
  "devDependencies": {
    "@types/chai": "4.3.4",
    "@types/mocha": "10.0.1",
    "@types/node": "16.18.24",
    "@types/sinon": "10.0.14",
    "chai": "4.3.7",
    "coveralls": "3.1.1",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "rimraf": "5.0.0",
    "sinon": "15.0.4",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.ts",
      "lib",
      "lib.es6"
    ]
  }
}
